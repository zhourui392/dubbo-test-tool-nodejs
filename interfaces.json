{
  "services": [
    {
      "id": "user-service-001",
      "name": "用户服务",
      "dubbo_config": {
        "host": "127.0.0.1",
        "port": 20880,
        "timeout": 5000,
        "version": "1.0.0",
        "group": "user"
      },
      "interfaces": [
        {
          "id": "user-interface-001",
          "name": "com.company.service.user.UserService",
          "methods": [
            {
              "id": "get-user-info",
              "name": "getUserInfo",
              "params": "{\"userId\": \"123456\"}",
              "return_type": "UserDTO",
              "description": "根据用户ID获取用户信息"
            },
            {
              "id": "update-user",
              "name": "updateUser",
              "params": "{\"userId\": \"123456\", \"name\": \"张三\", \"email\": \"zhangsan@example.com\"}",
              "return_type": "boolean",
              "description": "更新用户信息"
            },
            {
              "id": "create-user",
              "name": "createUser",
              "params": "{\"name\": \"李四\", \"email\": \"lisi@example.com\", \"phone\": \"13800138000\"}",
              "return_type": "String",
              "description": "创建新用户"
            }
          ]
        }
      ]
    }
  ]
}